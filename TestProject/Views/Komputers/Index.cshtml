@model IEnumerable<TestProject.Models.Komputer>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.System)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Monitor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Rozdzielczosc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CzyDziala)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.System)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Monitor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Rozdzielczosc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CzyDziala)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
